mixin product()
    .product&attributes(attributes)
        .product__image
            img(src="" alt="Imagen del producto" loading="lazy")
            span.product__discount         
        .product__info
            h4.product__name
            .product__prices
                h5.product__prices-before
                h5.product__prices-new

mixin line(imgUrl, nombre, lineaUrl)
    .line
        img(loading="lazy" src=""+imgUrl, alt="Imagen de la línea"+nombre).line__image
        a(href=""+lineaUrl).line__container 
            h3.line__container-name #{nombre}
